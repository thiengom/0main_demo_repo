VIEW localized_sap_capire_bookshop_Authors AS SELECT
  L.createdAt,
  L.createdBy,
  L.modifiedAt,
  L.modifiedBy,
  L.ID,
  L.name,
  L.dateOfBirth,
  L.dateOfDeath,
  L.placeOfBirth,
  L.placeOfDeath
FROM sap_capire_bookshop_Authors AS L
WITH ASSOCIATIONS (
  MANY TO MANY JOIN localized_sap_capire_bookshop_Books AS books ON (books.author_ID = ID)
)